{
    "description": "RAML-JSON-API PHP-code generator (based on RAML-types) for different FrameWorks aka Laravel/Yii/Symfony etc, with complete support of JSON-API data format",
    "type": "rjapi-raml",
    "name": "rjapi/raml-json-api",
    "require": {
        "php": "^7.0",
        "codeception/codeception": "^2.2",
        "alecsammon/php-raml-parser": "^2.1",
        "laravel/framework": "^5.3",
        "zendframework/zend-diactoros": "^1.3",
        "woohoolabs/yin": "^1.0",
        "seld/jsonlint": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "rjapi\\": "src",
            "rjapitest\\": "tests"
        }
    }
}
